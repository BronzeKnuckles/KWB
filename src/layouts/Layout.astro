---
interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en" data-theme="light">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width" />
		<title>{title}</title>
	</head>
	<body>
		<div class="navbar">
			<div class="navbar-start">
			  
			  <a class="btn btn-ghost text-3xl">Kingston Web Boys</a>
			</div>

			<div class="navbar-end">
			  <a class="btn text-lg">Pricing</a>
			  <a class = "btn text-lg">About</a>
			  <a class="btn text-lg">Contact Us</a>
			</div>
		  </div>
		<slot />
	</body>
</html>
